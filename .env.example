# Copy this file to .env and fill in the values.
# Values here will be loaded by scripts/sync_rules.py (unless already set in your shell).

# Cloudflare API token with: Zone -> Zone WAF -> Edit & Zone -> Zone WAF -> Read
CLOUDFLARE_API_TOKEN=cf_test_token_here

# Cloudflare Zone ID to manage
CLOUDFLARE_ZONE_ID=your_zone_id_here

# Shared config (used by all scripts). These are defaults for local dev.
# In CI (Jenkins), set these as job/env variables instead of using .env.

# Input CSV path
ASN_CSV=asn.csv

# Rule generation
RULE_TEMPLATE=rule-template.wf
RULES_DIR=rules

# Cloudflare limit is 4096
MAX_RULE_CHARS=4096

# CSV validation behavior
DUP_LIMIT=10
VALIDATE_JSON=false

# Sync behavior
DRY_RUN=false
